type User {
  id: ID! @id
  username: String!
  bio: String
  posts: [Post!]!
  likes: [Like!]!
  comments: [Comment!]!
}

type Post {
  location: String
  caption: String!
  user: User!
  files: [File!]!
  likes: [Like!]!
  comments: [Comment!]!
}

type Like {
  user: User!
  post: Post!
}

type Comment {
  text: String!
  user: User!
  post: Post!
}

type File {
  url: String!
  post: Post!
}